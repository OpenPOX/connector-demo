# 服务器配置
PORT=6767
ENV=development
SESSION_SECRET=your-session-secret-key-here

# 用于 OAuth 授权完成后跳转的前端页面地址
FRONTEND_AUTH_REDIRECT_URL=

# Google OAuth配置
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Slack OAuth配置
SLACK_CLIENT_ID=your-slack-client-id
SLACK_CLIENT_SECRET=your-slack-client-secret
SLACK_SIGNING_SECRET=your-slack-signing-secret

# Confluence OAuth配置
CONFLUENCE_CLIENT_ID=your-confluence-client-id
CONFLUENCE_CLIENT_SECRET=your-confluence-client-secret

# 重定向URL配置
REDIRECT_URL=https://your-domain.com

# 测试Token配置（可选，仅开发环境）
ENABLE_TEST_TOKENS=false
TEST_TOKEN_GOOGLE_ACCESS=your-test-google-access-token
TEST_TOKEN_GOOGLE_REFRESH=your-test-google-refresh-token
TEST_TOKEN_SLACK_ACCESS=your-test-slack-access-token
TEST_TOKEN_SLACK_REFRESH=your-test-slack-refresh-token
